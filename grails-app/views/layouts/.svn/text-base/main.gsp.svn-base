<html>
<head>
<title><g:layoutTitle default="Watchful Fox" /></title>
<link rel="stylesheet" href="${resource(dir:'css',file:'main.css')}" />
<link rel="shortcut icon"
	href="${resource(dir:'images',file:'favicon.ico')}" type="image/x-icon" />
<link rel="stylesheet"
	href="${createLinkTo(dir:'js', file: 'dojo-release-1.4.3-src/dijit/themes/tundra/tundra.css')}">
<link rel="stylesheet"
	href="${createLinkTo(dir:'js', file: 'dojo-release-1.4.3-src/dojo/resources/dojo.css')}">
<g:layoutHead />
<g:javascript library="application" />
</head>
<body class="tundra">
<div id="spinner" class="spinner" style="display: none;"><img
	src="${resource(dir:'images',file:'spinner.gif')}"
	alt="${message(code:'spinner.alt',default:'Loading...')}" /></div>
<div id="watchfulFoxLogo" name="watchfulFoxLogo"><a href="http://watchfulfox.com"><img
	src="${resource(dir:'images',file:'fox.gif')}" alt="Watchful Fox"
	border="0" /></a></div>
<g:layoutBody />
</body>
</html>